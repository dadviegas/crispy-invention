{
  "name": "site",
  "description": "Site app description",
  "version": "1.0.0",
  "main": "index.js",
  "author": "dadviegas",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "webpack",
    "start": "webpack-dev-server --hot --inline --mode=development",
    "start:dev": "webpack-dev-server --hot --inline --mode=development --content-base=./assets",
    "start:production": "webpack-dev-server --hot --inline --mode=production",
    "build:dev": "yarn clean & webpack --mode=development",
    "build:production": "yarn clean & webpack --mode=production --env.country=pt",
    "build:all": "yarn clean & webpack --config=webpack.config.all.babel.js --profile"
  },
  "dyarependencies": {
    "@dadv/react-bundle": "0.0.1",
    "@dadv/webpack-conf": "0.0.2",
    "@dadv/yaml-conf": "0.0.1"
  }
}
